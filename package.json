{
  "name": "moviesApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npm run test && react-native run-android",
    "ios": "react-native run-ios",
    "start": "npm run test && react-native start",
    "test": "jest",
    "lint": "eslint .",
    "version": "npm run build-android-debug && npm run changelog && git add CHANGELOG.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean-cache": "react-native start --reset-cache",
    "build-android-debug": "cd android && ./gradlew assembleDebug && cd .."
  },
  "dependencies": {
    "axios": "0.19.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-redux": "7.1.3",
    "react-router-native": "5.1.2",
    "redux-actions": "2.6.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "0.0.7",
    "babel-jest": "25.1.0",
    "conventional-changelog-cli": "2.0.31",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.58.0",
    "react-native-dotenv": "0.2.0",
    "react-test-renderer": "16.9.0",
    "reactotron-react-native": "4.0.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|my-project|react-native-button|react-router-native|react-router|)/)"
    ],
    "globals": {
      "window": {}
    }
  }
}
